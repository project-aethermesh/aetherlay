{
  "mainnet": {
    "llama-1": {
      "provider": "llama",
      "role": "primary",
      "type": "full",
      "http_url": "https://eth.llamarpc.com",
      "rate_limit_recovery": {
        "backoff_multiplier": 2.0,
        "initial_backoff": 30,
        "max_backoff": 300,
        "max_retries": 10,
        "required_successes": 3,
        "reset_after": 3600
      }
    },
    "drpc-1": {
      "provider": "drpc",
      "role": "fallback",
      "type": "full",
      "http_url": "https://eth.drpc.org",
      "ws_url": "wss://eth.drpc.org"
    },
    "publicnode-1": {
      "provider": "publicnode",
      "role": "fallback",
      "type": "archive",
      "http_url": "https://ethereum-rpc.publicnode.com",
      "ws_url": "wss://ethereum-rpc.publicnode.com"
    }
  },
  "arbitrum": {
    "drpc-1": {
      "provider": "drpc",
      "role": "primary",
      "type": "full",
      "http_url": "https://arbitrum.drpc.org"
    },
    "publicnode-1": {
      "provider": "public_node",
      "role": "fallback",
      "type": "archive",
      "http_url": "https://arbitrum-one-rpc.publicnode.com",
      "ws_url": "wss://arbitrum-one-rpc.publicnode.com"
    }
  },
  "base": {
    "alchemy-test": {
      "provider": "alchemy",
      "role": "primary",
      "type": "archive",
      "http_url": "https://base-mainnet.g.alchemy.com/v2/${ALCHEMY_API_KEY}",
      "ws_url": "wss://base-mainnet.g.alchemy.com/v2/${ALCHEMY_API_KEY}",
      "rate_limit_recovery": {
        "backoff_multiplier": 2.0,
        "initial_backoff": 30,
        "max_backoff": 300,
        "max_retries": 10,
        "required_successes": 3,
        "reset_after": 3600
      }
    },
    "infura-staging": {
      "provider": "infura",
      "role": "primary",
      "type": "archive",
      "http_url": "https://base-mainnet.infura.io/v3/${INFURA_API_KEY}",
      "ws_url": "wss://base-mainnet.infura.io/ws/v3/${INFURA_API_KEY}",
      "rate_limit_recovery": {
        "backoff_multiplier": 2.0,
        "initial_backoff": 30,
        "max_backoff": 300,
        "max_retries": 10,
        "required_successes": 3,
        "reset_after": 3600
      }
    }
  }
